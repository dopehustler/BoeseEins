/*  
* BoeseEins - Simples Würfelspiel  
* @author  Cédric Tschudin
* @version 1.3 
*/

import java.util.Random;


/*
 * Klasse liefert Zufallszahl zwischen 1 und 6
 */
public class GameBoeseEins{

	int wurf;
	Random rand = new Random();


	public int wuerfe(){

		wurf = rand.nextInt(6) + 1;
		
		
		return wurf;
		
	}

}
